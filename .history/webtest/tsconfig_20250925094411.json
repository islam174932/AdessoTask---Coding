{
  "compilerOptions": {
    "target": "ESNext",
  "module": "NodeNext",
  "moduleResolution": "NodeNext",
    "outDir": "dist",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true,
  "allowImportingTsExtensions": true,
  "noEmit": true
  },
  "include": ["src/**/*.ts", "features/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
